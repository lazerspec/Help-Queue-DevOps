events {}

http {
    server {
        listen 80;
        location / {
            proxy_pass http://${TICKET_UI_CLUSTER_IP}:3000;
        }
        location /api/ {
            proxy_pass http://${TICKET_API_CLUSTER_IP}:8081;
        }
        location /manage {
            proxy_pass http://${TICKET_API_CLUSTER_IP}:8081;
        }
    }
}